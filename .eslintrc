{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "prettier",
    "prettier/react"
  ],
  "overrides": [],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": [
      "warn",
      {"extensions": [".js", ".jsx"]}
    ],
    "import/prefer-default-export": "off",
    "jsx-quotes": ["error", "prefer-single"]
  }
}